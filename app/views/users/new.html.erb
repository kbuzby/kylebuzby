<div class="container">
  <%= form_for @user, html: {multipart: true} do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name %><br/>
    <%=f.label :email %>
    <%=f.text_field :email %><br/>
    <%=f.label :password %>
    <%=f.password_field :password %><br/>
    <%=f.submit 'Submit' %>
  <% end %>
</div>
